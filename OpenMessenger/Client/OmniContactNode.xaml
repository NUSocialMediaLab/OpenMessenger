<UserControl x:Class="OpenMessenger.Client.OmniContactNode"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="150" Width="150">
    <Grid>
        <Ellipse Name="halo" Stroke="Black" StrokeThickness="0" />
        <Rectangle Name="info1Box" Stroke="Gray" StrokeThickness="0" HorizontalAlignment="Right" Margin="0,0,-110,0" Width="127" Height="29" VerticalAlignment="Top" />
        <Label Name="info1" FontSize="15" HorizontalAlignment="Right" Margin="0,0,-110,0" Width="127" Height="29" VerticalAlignment="Top">Info</Label>
        <Rectangle Name="info2Box" Stroke="Gray" StrokeThickness="0" HorizontalAlignment="Right" Margin="0,47,-141,74" Width="127" />
        <Label Name="info2" FontSize="15" HorizontalAlignment="Right" Margin="0,47,-141,74" Width="127">Info</Label>
        <Rectangle Name="info0Box" Stroke="Gray" StrokeThickness="0" HorizontalAlignment="Right" Margin="0,0,-110,23" Width="127" Height="29" VerticalAlignment="Bottom" />
        <Label Name="info0" FontSize="15" HorizontalAlignment="Right" Margin="0,0,-110,23" Width="127" Height="29" VerticalAlignment="Bottom">Info</Label>
        <Label Name="label" FontSize="20" VerticalAlignment="Center" HorizontalContentAlignment="Center">Contact Name</Label>
    </Grid>
</UserControl>
