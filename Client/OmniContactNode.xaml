<UserControl x:Class="OpenMessenger.Client.OmniContactNode"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="100" Width="100" Opacity="1">
    <Grid>
        <Ellipse Name="halo" StrokeThickness="0" Opacity="1" Fill="White" OpacityMask="White" Stroke="White"></Ellipse>
        <Border  Name="infoBox" HorizontalAlignment="Right" Margin="0,-9.65,-110,0" Width="102" BorderThickness="0" CornerRadius="10,10,10,10" Background="White" Height="50" VerticalAlignment="Top">
            <Label Name="info" FontSize="12" Width="67" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right"></Label>
        </Border>
        <Label Name="label" FontSize="20" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0">Contact Name</Label>
        <Image Margin="0,-8.65,-42,0" Name="keyboardImg" Stretch="Fill" HorizontalAlignment="Right" Width="25" Height="25" VerticalAlignment="Top" ClipToBounds="False" Opacity="1">
        </Image>
        <Image ClipToBounds="False" Height="25" HorizontalAlignment="Right" Margin="0,15.35,-42,0" Name="micImg" Opacity="1" Stretch="Fill" VerticalAlignment="Top" Width="25" />
    </Grid>
</UserControl>
